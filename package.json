{
  "name": "guardian-auth-narak",
  "version": "1.0.6",
  "description": "Sistema encapsulado de autenticação para React com suporte a tema claro/escuro",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "style": "dist/guardian-auth.css",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./dist/guardian-auth.css": "./dist/guardian-auth.css"
  },
  "files": [
    "dist",
    "dist/guardian-auth.css"
  ],
  "scripts": {
    "build": "rollup -c && node scripts/build-css.js",
    "build:js": "rollup -c",
    "build:css": "node scripts/build-css.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "react",
    "authentication",
    "auth",
    "login",
    "register",
    "password-recovery",
    "protected-routes",
    "theme-toggle"
  ],
  "author": "Karan Luciano",
  "license": "MIT",
  "peerDependencies": {
    "axios": "^1.8.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "rollup": "^3.29.5",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lkaranl/guardian-auth.git"
  },
  "bugs": {
    "url": "https://github.com/lkaranl/guardian-auth/issues"
  },
  "homepage": "https://github.com/lkaranl/guardian-auth/blob/main/README.md"
}
